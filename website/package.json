{
  "name": "booth",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --port 3000 --mode development",
    "stg": "vite --port 80 --mode staging",
    "prd": "vite --port 80 --mode production",
    "build:dev": "pnpm run tsc && vite build --mode development",
    "build:stg": "pnpm run tsc && vite build --mode staging",
    "build": "pnpm run tsc && vite build --mode production",
    "visualizer": "pnpm run tsc && VISUALIZER=true vite build --mode production",
    "release": "pnpm exec tsx scripts/release.ts",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --fix",
    "tsc": "tsc --noEmit",
    "prettier": "prettier -c --write src/**/*.{ts,tsx}",
    "pre-commit": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "pnpm dlx playwright test"
  },
